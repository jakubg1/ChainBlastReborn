{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "description": "The root object.",
    "properties": {
        "$schema": true,
        "timeline": {
            "type": "array",
            "description": "Defines a Timeline consisting of Timeline Entries.",
